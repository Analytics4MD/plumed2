q1: Q1 SPECIESA=1,2 SPECIESB=1-64 D_0=3.0 R_0=1.5 NN=12 SUM

dd: DOTPRODUCT_MATRIX ...
     GROUP1=q1.rm-[-1] 
     GROUP2=q1.im-[-1] 
     GROUP3=q1.rm-[0]  
     GROUP4=q1.im-[0]  
     GROUP5=q1.rm-[1]  
     GROUP6=q1.im-[1]  
...

cc: COORDINATIONNUMBER WEIGHT=dd 
ff: COMBINE ARG=cc PERIODIC=NO

DUMPDERIVATIVES ARG=ff FILE=deriv

BIASVALUE ARG=ff 
PRINT ARG=q1.*,ff FILE=colvar FMT=%8.4f 
